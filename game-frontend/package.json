    "test": "ng test",
    "createVersionFile": "node scripts/create-version-file.js",
    "compileChangelog": "node scripts/compile-changelog.js",
    "setVersion": "node scripts/set-project-version.js",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor"
