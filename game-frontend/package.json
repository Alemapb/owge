{
  "name": "game-frontend",
  "version": "0.8.1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "postinstall": "npm run createVersionFile",
    "ng": "ng",
    "start": "npm run createVersionFile && npm run compileChangelog && ng serve",
    "startAdmin": "ng serve game-admin",
    "buildLibs": "ng build owge-core && ng build owge-widgets && ng build owge-universe && ng build owge-alliance && ng build owge-galaxy && ng build owge-faction",
    "ngServeDocker": "npm run createVersionFile && npm run compileChangelog && ng serve --host 0.0.0.0 --disable-host-check",
    "ngServeDockerAdmin": "ng serve game-admin --host 0.0.0.0 --disable-host-check",
    "build": "npm run buildLibs && npm run createVersionFile && npm run compileChangelog && ng build --prod  && npm run addVersionSuffixWebpackBundles",
    "buildAdmin": "npm run buildLibs && npm run createVersionFile && npm run compileChangelog && ng build game-admin --prod  && npm run addVersionSuffixWebpackBundles -- dist/game-admin/index.html",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "createVersionFile": "node scripts/create-version-file.js",
    "compileChangelog": "node scripts/compile-changelog.js",
    "addVersionSuffixWebpackBundles": "node scripts/add-version-suffix-webpack-bundles.js",
    "setVersion": "node scripts/set-project-version.js",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor"
  },
  "owge": {
    "versionDate": "now"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/architect": "^0.803.4",
    "@angular/animations": "^8.2.2",
    "@angular/common": "^8.2.2",
    "@angular/compiler": "^8.2.2",
    "@angular/core": "^8.2.2",
    "@angular/forms": "^8.2.2",
    "@angular/platform-browser": "^8.2.2",
    "@angular/platform-browser-dynamic": "^8.2.2",
    "@angular/router": "^8.2.2",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@ng-bootstrap/ng-bootstrap": "^5.1.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "angular2-fontawesome": "~5.2.0",
    "bootstrap": "^4.4.1",
    "core-js": "^2.4.1",
    "jquery": "^3.1.1",
    "karma-spec-reporter": "0.0.26",
    "lodash-es": "^4.17.15",
    "ng2-logger": "^2.0.48",
    "popper.js": "^1.14.1",
    "rxjs": "^6.5.2",
    "socket.io-client": "^2.2.0",
    "ts-helpers": "^1.1.1",
    "ts-mixer": "^4.0.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.4",
    "@angular-devkit/build-ng-packagr": "^0.803.4",
    "@angular/cli": "^8.3.4",
    "@angular/compiler-cli": "^8.2.2",
    "@angular/language-service": "^8.2.2",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "@types/socket.io-client": "^1.4.29",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jsdom": "^11.12.0",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-less-preprocessor": "^0.3.3",
    "ng-packagr": "^5.3.0",
    "protractor": "~5.1.2",
    "replace-in-file": "^4.2.0",
    "showdown": "^1.8.6",
    "ts-node": "~4.1.0",
    "tsickle": "^0.36.0",
    "tslint": "~5.9.1",
    "typescript": "~3.5.3"
  }
