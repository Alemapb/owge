    "owge-widgets": {
      "projectType": "library",
      "root": "projects/owge-widgets",
      "sourceRoot": "projects/owge-widgets/src",
      "prefix": "owge-widgets",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
