{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "game-frontend": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/@fortawesome/fontawesome-free/css/fontawesome.css",
              "node_modules/@fortawesome/fontawesome-free/css/solid.css",
              "src/styles.less",
              "src/forms.less"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/popper.js/dist/umd/popper.js",
              "node_modules/bootstrap/dist/js/bootstrap.js"
            ]
          },
          "configurations": {
            "publicAccount": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.public-account.ts"
                }
              ]
            },
            "localDrupal": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/local-drupal-account.environment.ts"
                }
              ]
            },
            "becariaccount": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.becariaccount.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "game-frontend:build"
          },
          "configurations": {
            "publicAccount": {
              "browserTarget": "game-frontend:build:publicAccount"
            },
            "localDrupal": {
              "browserTarget": "game-frontend:build:localDrupal"
            },
            "becariaccount": {
              "browserTarget": "game-frontend:build:becariaccount"
            },
            "production": {
              "browserTarget": "game-frontend:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "game-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/popper.js/dist/umd/popper.js",
              "node_modules/bootstrap/dist/js/bootstrap.js"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/@fortawesome/fontawesome-free/css/fontawesome.css",
              "node_modules/@fortawesome/fontawesome-free/css/solid.css",
              "src/styles.less",
              "src/forms.less"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "game-frontend-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "game-frontend:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "game-admin": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/game-admin",
      "sourceRoot": "projects/game-admin/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/game-admin",
            "index": "projects/game-admin/src/index.html",
            "main": "projects/game-admin/src/main.ts",
            "polyfills": "projects/game-admin/src/polyfills.ts",
            "tsConfig": "projects/game-admin/tsconfig.app.json",
            "aot": false,
            "assets": [
              "projects/game-admin/src/favicon.ico",
              "projects/game-admin/src/assets"
            ],
            "styles": [
              "projects/game-admin/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "libs": {
              "tsConfig": "projects/game-admin/tsconfig.libs.json"
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/game-admin/src/environments/environment.ts",
                  "with": "projects/game-admin/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "game-admin:build"
          },
          "configurations": {
            "libs": {
              "browserTarget": "game-admin:build:libs"
            },
            "production": {
              "browserTarget": "game-admin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "game-admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/game-admin/src/test.ts",
            "polyfills": "projects/game-admin/src/polyfills.ts",
            "tsConfig": "projects/game-admin/tsconfig.spec.json",
            "karmaConfig": "projects/game-admin/karma.conf.js",
            "assets": [
              "projects/game-admin/src/favicon.ico",
              "projects/game-admin/src/assets"
            ],
            "styles": [
              "projects/game-admin/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/game-admin/tsconfig.app.json",
              "projects/game-admin/tsconfig.spec.json",
              "projects/game-admin/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/game-admin/e2e/protractor.conf.js",
            "devServerTarget": "game-admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "game-admin:serve:production"
            }
          }
        }
      }
    },
    "owge-core": {
      "projectType": "library",
      "root": "projects/owge-core",
      "sourceRoot": "projects/owge-core/src",
      "prefix": "owge-core",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/owge-core/tsconfig.lib.json",
            "project": "projects/owge-core/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/owge-core/src/test.ts",
            "tsConfig": "projects/owge-core/tsconfig.spec.json",
            "karmaConfig": "projects/owge-core/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/owge-core/tsconfig.lib.json",
              "projects/owge-core/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
